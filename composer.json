{
   "name": "elie29/oci-driver",
   "description": "OCI Driver",
   "license": "MIT",
   "keywords": [],
   "authors": [
      {
         "name": "Elie NEHME",
         "email": "elie29@gmail.com"
      }
   ],
   "support": {
      "issues": "https://github.com/elie29/oci-driver/issues",
      "source": "https://github.com/elie29/oci-driver"
   },
   "type": "library",
   "minimum-stability": "stable",
   "config": {
      "sort-packages": true,
      "process-timeout": 9000
   },
   "require": {
      "php": "^7.1"
   },
   "require-dev": {
      "hamcrest/hamcrest-php": "^2.0",
      "mayflower/php-codebrowser": "^2.0",
      "mockery/mockery": "^1.0",
      "pdepend/pdepend": "^2.5",
      "phploc/phploc": "^4.0",
      "phpmd/phpmd": "^2.6",
      "phpmetrics/phpmetrics": "^2.4",
      "phpstan/phpstan": "^0.10.3",
      "phpstan/phpstan-mockery": "^0.10.2",
      "phpunit/phpunit": "^7.1",
      "sebastian/phpcpd": "^4.1",
      "symfony/var-dumper": "^4.0",
      "zendframework/zend-coding-standard": "^1.0"
   },
   "autoload": {
      "psr-4": {
         "": "src/"
      }
   },
   "autoload-dev": {
      "files": [
         "vendor/hamcrest/hamcrest-php/hamcrest/Hamcrest.php"
      ],
      "psr-4": {
         "": "tests/"
      }
   },
   "scripts": {
      "cover": [
         "@test",
         "php -S localhost:5000 -d display_errors=1 -t build/coverage"
      ],
      "test": [
         "@clean",
         "phpunit"
      ],
      "clean": [
         "rm -rf build/*"
      ],
      "check": [
         "@cs-check",
         "@test"
      ],
      "cs-check": "phpcs",
      "cs-fix": "phpcbf"
   }
}
